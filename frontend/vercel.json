{
  "version": 2,
  "name": "edutrack-frontend",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://edutrack-r3kn.onrender.com",
    "VITE_APP_ENV": "production"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://edutrack-r3kn.onrender.com",
      "VITE_APP_ENV": "production"
    }
  }
}