{
  "version": 2,
  "name": "edutrack-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://edutrack-r3kn.onrender.com",
    "VITE_APP_ENV": "production"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://edutrack-r3kn.onrender.com",
      "VITE_APP_ENV": "production"
    }
  }
}